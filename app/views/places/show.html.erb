<h1><%= @place["name"]%></h1>


<ul>
  <% for post in @posts %>
    <h3> <%= post["title"] %> </h3> 
    <h5> <%= post["posted_on"]%> </h5> 
    <%= post["description"] %>
  <% end %>
</ul>


<p>
  <a href="/posts/new?place_id=<%= @place["id"] %>">New Post</a>
</p>


<p>
    <a href="/places">Back to all places</a>
</p>